#include <bits/stdc++.h>
using namespace std;

int compareVersions(string a, string b) 
{
    int n1 = a.size();
    int n2 = b.size();
    int i  = 0;
    int j = 0;

    while(i<n1 || j<n2){
        long num1 = 0 , num2  = 0;

        // extract number from a
        while(i<n1 && a[i] != '.'){
            num1 = num1*10 + (a[i]-'0');
            i++;
        }

        // extract number from b
        while(j<n2 && b[j] != '.'){   // âœ… fixed to b[j]
            num2 = num2*10 + (b[j]-'0');
            j++;
        }

        if(num1 > num2) return 1;
        if(num1 < num2) return -1;

        // skip dot if present
        i++;
        j++;
    }
    return 0;
}
