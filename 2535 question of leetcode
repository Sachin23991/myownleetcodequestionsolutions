class Solution {
public:
    int differenceOfSum(vector<int>& nums) {
        int elementSum = 0;
        int digitSum = 0;

        for (int num : nums) {
            elementSum += num;
            int x = num;
            while (x > 0) {       // extract all digits
                digitSum += x % 10;
                x /= 10;
            }
        }

        return abs(elementSum - digitSum);
    }
};
