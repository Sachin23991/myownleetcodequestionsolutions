class Solution {
public:
    bool isNoZero(int x){
        // we have to find that the number is non zero
    while(x > 0){
        if(x%10 == 0) return false;

        x/=10;
    }

        return true;
    }
    vector<int> getNoZeroIntegers(int n) {
        for(int i  = 1 ; i< n;i++){
            int j  = n-i;

            if (isNoZero(i) && isNoZero(j)) {
                return {i, j};
            }
        }
        return {};
    }
};
